<h1>Your questions</h1>
<div class="questions-container">
  <% if (questions.length === 0) { %>
    <%= 'You have no questions! :(' %>
  <% } else { %>
    <% questions.forEach(function (question) { %>
      <div class="question-item">
        <a href="#/questions/<%= question.id %>"><%= question.escape('title') %></a>
        <a class="edit" data-toggle="tooltip" data-original-title="Edit" href="#/questions/<%= question.id %>/edit"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span></a>
        <a class="delete" data-toggle="tooltip" data-original-title="Delete" data-id="<%= question.id %>" href="javascript:void(0)"><span class="glyphicon glyphicon-remove" aria-hidden="true"></span></a>
      </div>
    <% }) %>
  <% } %>
</div>
